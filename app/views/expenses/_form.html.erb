<%= simple_form_for([@profile, @expense], :url => profile_expense_path(@profile)) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :clothing %>
    <%= f.input :healthcare %>
    <%= f.input :vehicle %>
    <%= f.input :home_maintenance %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
